<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from MoedaEstrangeiraService WSDL service -->

  <service name="MoedaEstrangeira" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://loja.fiap.com.br"/>
      <parameter name="wsdlServiceElement" value="MoedaEstrangeiraService"/>
      <parameter name="schemaQualified" value="http://loja.fiap.com.br"/>
      <parameter name="wsdlServicePort" value="MoedaEstrangeira"/>
      <parameter name="className" value="br.com.fiap.loja.MoedaEstrangeiraSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="MoedaEstrangeira"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="converterDolar" qname="operNS:converterDolar" xmlns:operNS="http://loja.fiap.com.br" returnQName="retNS:converterDolarReturn" xmlns:retNS="http://loja.fiap.com.br" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:valor" xmlns:pns="http://loja.fiap.com.br" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="converterDolar"/>

  </service>
</deployment>
